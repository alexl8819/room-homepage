{
  "name": "room-homepage-master",
  "private": true,
  "version": "1.0.0",
  "description": "![Design preview for the Room homepage coding challenge](./design/desktop-preview.jpg)",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "build": "parcel build --public-url ./ --dist-dir public",
    "lint": "npx eslint ./src --max-warnings=0",
    "test": "npx jest --no-cache"
  },
  "author": "alexl8819",
  "license": "",
  "devDependencies": {
    "@angular-eslint/template-parser": "^16.1.0",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@fontsource/league-spartan": "^5.0.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-jest": "^29.6.1",
    "eslint": "^8.41.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^22.1.0",
    "parcel": "^2.9.1",
    "tailwindcss": "^3.3.2",
    "text-encoding": "^0.7.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/src/setupFiles.js"],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|svg|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  }
